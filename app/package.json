{
  "name": "electron_startup",
  "version": "1.0.0",
  "author": "rosendo",
  "license": "MIT",
  "type": "commonjs",
  "description": "electron template",
  "main": "main.js",
  "scripts": {
    "build:preload": "node preload/build.js",
    "prestart": "npm run build:preload",
    "start": "env-cmd -f ../env/.env electron-forge start",
    "start:watch": "nodemon npm run start",
    "preprod": "npm run build:preload",
    "prod": "env-cmd -f ../env/.env.prod electron-forge start",
    "package": "electron-forge package",
    "premake": "env-cmd -f ../env/.env.prod npm run build:preload",
    "make": "env-cmd -f ../env/.env.prod electron-forge make"
  },
  "dependencies": {
    "@types/node": "20.5.1",
    "cheerio": "1.0.0-rc.12",
    "dayjs": "1.11.9",
    "env-cmd": "10.1.0",
    "glob": "10.3.3",
    "koa-compose": "4.1.0",
    "level": "8.0.0",
    "log4js": "6.9.1",
    "make-dir": "4.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.3.0",
    "electron": "^26.0.0",
    "nodemon": "^3.0.1"
  }
}
