{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-unused-labels": "off",
    "indent": ["off", "tab"],
    "linebreak-style": ["error", "unix"],
    "quotes": ["off", "single"],
    "no-case-declarations": "off",
    "semi": ["error", "always"]
  }
}
